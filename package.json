{
  "name": "peek-cursor",
  "displayName": "peek-cursor",
  "description": "peek cursor or selection out of visible area.",
  "version": "0.0.1",
  "publisher": "awtnb",
  "icon": "images/logo.png",
  "repository": {
    "url": "https://github.com/AWtnb/vscode-peek-cursor"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:peek-cursor.peekNext",
    "onCommand:peek-cursor.peekPrevious"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "peek-cursor.peekNext",
        "title": "peek-cursor: to Next"
      },
      {
        "command": "peek-cursor.peekPrevious",
        "title": "peek-cursor: to Previous"
      }
    ],
    "keybindings": [
      {
        "command": "peek-cursor.peekNext",
        "key": "ctrl+k ctrl+n",
        "when": "editorTextFocus"
      },
      {
        "command": "peek-cursor.peekPrevious",
        "key": "ctrl+k ctrl+shift+n",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
